<mvc:View
	xmlns:form="sap.ui.layout.form"
	xmlns:table="sap.ui.table"
	controllerName="com.ingenx.nauti.submitquotation.controller.Bidding"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m"
	xmlns:l="sap.ui.layout"
	displayBlock="true"
	height="100%"
	busyIndicatorDelay="0"
>
	<Page id="_IDGenPage1" showHeader="false">
		<l:VerticalLayout id="_IDGenVerticalLayout1"
			width="100%"
			class="sapUiContentPadding"
		>
			<l:BlockLayout id="_IDGenBlockLayout1" background="Dashboard">
				<l:BlockLayoutRow id="_IDGenBlockLayoutRow1">
					<l:BlockLayoutCell id="_IDGenBlockLayoutCell1" title="Details for Charter: 2000001">
						<l:HorizontalLayout id="_IDGenHorizontalLayout1" allowWrapping="false">
							<form:SimpleForm id="_IDGenSimpleForm1"
								editable="false"
								backgroundDesign="Transparent"
								layout="ColumnLayout"
							>
								<Label id="_IDGenLabel1"
									text="Voyage Type"
									labelFor="voyageType"
									design="Bold"
								/>
								<Text
									text="Voyage Charter"
									id="voyageType"
								/>
								<Label id="_IDGenLabel2"
									text="Vessel Type"
									labelFor="vesselType"
									design="Bold"

								/>
								<Text
									text="1000"
									id="vesselType"
								/>
							</form:SimpleForm>
							<form:SimpleForm id="_IDGenSimpleForm2"
								editable="false"
								backgroundDesign="Transparent"
								layout="ColumnLayout"
							>
								<Label id="_IDGenLabel3"
									text="Bid Start"
									labelFor="bidStart"
									design="Bold"
								/>
								<Text
									text="2024-05-15 10:15:00"
									id="bidStart"
								/>
								<Label id="_IDGenLabel4"
									text="Bid End"
									labelFor="bidEnd"
									design="Bold"
								/>
								<Text
									text="2024-05-15 10:55:00"
									id="bidEnd"
								/>
							</form:SimpleForm>
							<form:SimpleForm id="_IDGenSimpleForm3"
								editable="false"
								backgroundDesign="Transparent"
								layout="ColumnLayout"
							>
								<Label id="_IDGenLabel5"
									text="Bidding Type"
									labelFor="biddingType"
									design="Bold"
								/>
								<Text
									text="1 bidding System"
									id="biddingType"
								/>
								<Label id="_IDGenLabel6"
									text="Currency"
									labelFor="currency"
									design="Bold"
								/>
								<Text
									text="INR"
									id="currency"
								/>
							</form:SimpleForm>
						</l:HorizontalLayout>
					</l:BlockLayoutCell>
				</l:BlockLayoutRow>
				<l:BlockLayoutRow id="_IDGenBlockLayoutRow2">
					<l:BlockLayoutCell id="_IDGenBlockLayoutCell2">
						<IconTabBar id="_IDGenIconTabBar1"
							expanded="true"
							expandable="false"
							tabDensityMode="Compact"
							backgroundDesign="Transparent"
						>
							<items>
								<IconTabFilter id="_IDGenIconTabFilter1" text="Voyage Details">
									<table:Table
										id="idPortTab"
										selectionMode="None"
										rows="{sample>biddingData}"
										visibleRowCountMode="Auto"
									>
										<table:extension>
											<OverflowToolbar id="_IDGenOverflowToolbar1" class="sapUiSmallMarginBeginEnd"/>
										</table:extension>
										<table:columns>
											<table:Column id="_IDGenColumn1"
												width="3.5rem"
												hAlign="Center"
											>
												<Label id="_IDGenLabel7" text="LegID"/>
												<table:template>
													<Input id="_IDGenInput1"
														value="12"
														editable="false"
														textAlign="Center"
													/>
												</table:template>
											</table:Column>
											<table:Column id="_IDGenColumn2" width="auto">
												<Label id="_IDGenLabel8" text="Port Code"/>
												<table:template>
													<Input id="_IDGenInput2"
														value="INBOM"
														editable="false"
													/>
												</table:template>
											</table:Column>
											<table:Column id="_IDGenColumn3" width="auto">
												<Label id="_IDGenLabel9" text="Port Name"/>
												<table:template>
													<Input id="_IDGenInput3"
														value="Portn"
														editable="false"
													/>
												</table:template>
											</table:Column>
											<table:Column id="_IDGenColumn4"
												width="auto"
												hAlign="Right"
											>
												<Label id="_IDGenLabel10" text="Cargo Size"/>
												<table:template>
													<Input id="_IDGenInput4"
														value="Cargs"
														editable="false"
														textAlign="Right"
													/>
												</table:template>
											</table:Column>
											<table:Column id="_IDGenColumn5" width="3.5rem">
												<Label id="_IDGenLabel11" text="Unit"/>
												<table:template>
													<Input id="_IDGenInput5"
														value="Cargu"
														editable="false"
													/>
												</table:template>
											</table:Column>
											<table:Column id="_IDGenColumn6"
												width="auto"
												hAlign="Right"
											>
												<Label id="_IDGenLabel12" text="Distance(NM)"/>
												<table:template>
													<Input id="_IDGenInput6"
														value="Pdist"
														editable="false"
														textAlign="Right"
													/>
												</table:template>
											</table:Column>
											<table:Column id="_IDGenColumn7"
												width="auto"
												hAlign="Right"
											>
												<Label id="_IDGenLabel13" text="Arrival Date"/>
												<table:template>
													<DatePicker id="_IDGenDatePicker1"
														editable="false"
														textAlign="Right"
													/>
												</table:template>
											</table:Column>
											<table:Column id="_IDGenColumn8" width="auto">
												<Label id="_IDGenLabel14" text="Arrival Time"/>
												<table:template>
													<Input id="_IDGenInput7"
														value="
															path: 
														"
														editable="false"
													/>
												</table:template>
											</table:Column>
											<table:Column id="_IDGenColumn9"
												width="auto"
												hAlign="Right"
											>
												<Label id="_IDGenLabel15" text="Departure Date"/>
												<table:template>
													<DatePicker id="_IDGenDatePicker2"
														editable="false"
														textAlign="Right"
													/>
												</table:template>
											</table:Column>
											<table:Column id="_IDGenColumn10" width="auto">
												<Label id="_IDGenLabel16" text="Departure Time"/>
												<table:template>
													<Input id="_IDGenInput8"
														value="
															path
														"
														editable="false"
													/>
												</table:template>
											</table:Column>
										</table:columns>
									</table:Table>
								</IconTabFilter>
								<IconTabFilter id="_IDGenIconTabFilter7"
									text="Vessel Details"
									visible="true"
								>
									<form:SimpleForm id="_IDGenSimpleForm9"
										editable="false"
										backgroundDesign="Transparent"
										width="120%"
									>
										<Label id="_IDGenLabel34" text="Vessel Name"/>
										<Text id="_IDGenText35"
											width="10rem"
											text="name"
										/>
										<Label id="_IDGenLabel36" text="Vessel IMO No"/>
										<Text id="_IDGenText37"
											width="10rem"
											text="Vimo"
										/>
									</form:SimpleForm>
								</IconTabFilter>
								
								<IconTabFilter id="_IDGenIconTabFilter4"
									text="Submit Vessel Details"
									visible="true"
								>
									<form:SimpleForm id="_IDGenSimpleForm6"
										editable="true"
										backgroundDesign="Transparent"
										width="120%"
									>
										<Label id="_IDGenLabel22"
											text="Vessel Name"
											labelFor="vesselName"
										/>
										<Input
											id="vesselName"
											width="10rem"
										/>
										<Label id="_IDGenLabel23"
											text="Vessel IMO No"
											labelFor="vesselIMONo"
										/>
										<Input
											id="vesselIMONo"
											width="10rem"
										/>
									</form:SimpleForm>
								</IconTabFilter>
								<IconTabFilter id="_IDGenIconTabFilter6"
									text="Bid Details"
								>
									<l:BlockLayout id="_IDGenBlockLayout4" background="Dashboard">
										<l:BlockLayoutRow id="_IDGenBlockLayoutRow5">
											<l:BlockLayoutCell id="_IDGenBlockLayoutCell7"
												title="Technical Details"
												titleAlignment="Center"
												titleLevel="Auto"
												width="1"
											>
												<Table
													alternateRowColors="true"
													id="techDetailTable"
													sticky="ColumnHeaders"
												>
													<columns>
														<Column id="_IDGenColumn11" width="42.5%">
															<Text id="_IDGenText11" text="Technical Details"/>
														</Column>
														<Column id="_IDGenColumn12" width="42.5%">
															<Text id="_IDGenText12" text="Value"/>
														</Column>
														<Column id="_IDGenColumn13" width="15%">
															<Text id="_IDGenText13"
																text="Pref. Req."
																tooltip="Preferred Requirements"
															/>
														</Column>
													</columns>
													<items>
														<ColumnListItem id="_IDGenColumnListItem1">
															<Text id="_IDGenText14" text="{CodeDesc}"/>
															<Text id="_IDGenText15" text="{Value}"/>
															<Button id="_IDGenButton1"
																icon="sap-icon://hint"
																press="_onHintPress"
															/>
														</ColumnListItem>
													</items>
												</Table>
												<!-- <table:Table
													id="bidDetailTable"
													selectionMode="None"
													visibleRowCountMode="Auto"
													alternateRowColors="true"
													editable="false"
												>
													<table:columns>
														<table:Column>
															<Label text="Technical Details"/>
															<table:template>
																<Text text="{CodeDesc}"/>
															</table:template>
														</table:Column>
														<table:Column>
															<Label text="Value"/>
															<table:template>
																<Text text="{Value}"/>
															</table:template>
														</table:Column>
													</table:columns>
												</table:Table> -->
											</l:BlockLayoutCell>
											<l:BlockLayoutCell id="_IDGenBlockLayoutCell8"
												title="Commercial Details"
												titleAlignment="Center"
												titleLevel="Auto"
												width="1"
											>
												<form:SimpleForm id="_IDGenSimpleForm8"
													editable="true"
													backgroundDesign="Transparent"
													width="50%"
												>
													<Label id="_IDGenLabel21"
														text="Freight cost(Lumpsum)"
														labelFor="fCost"
													/>
													<Input
														id="fCost"
														liveChange="fCostChange"
														editable="false"
													/>
													<Label id="_IDGenLabel32"
														text="Total Cost"
														labelFor="totalCost"
													/>
													<l:HorizontalLayout id="_IDGenHorizontalLayout4">
														<Input
															id="totalCost"
															editable="false"
															width="120%"
														/>
														<Label id="_IDGenLabel33"
															text="INR"
															design="Bold"
															labelFor="totalCost"
															class="sapUiTinyMarginBegin sapUiTinyMarginTop"
														/>
													</l:HorizontalLayout>
												</form:SimpleForm>
											</l:BlockLayoutCell>
										</l:BlockLayoutRow>
									</l:BlockLayout>
								</IconTabFilter>
								<IconTabFilter id="_IDGenIconTabFilter5"
									text="Submit Bid"
									visible="true"
								>
									<l:BlockLayout id="_IDGenBlockLayout3" background="Dashboard">
										<l:BlockLayoutRow id="_IDGenBlockLayoutRow4">
											<l:BlockLayoutCell id="_IDGenBlockLayoutCell5"
												title="Enter Technical Details"
												titleAlignment="Center"
												titleLevel="Auto"
											>
												<Table
													alternateRowColors="true"
													id="submitTechDetailTable"
													sticky="ColumnHeaders"
												>
													<columns>
														<Column id="_IDGenColumn14" width="42.5%">
															<Text id="_IDGenText8" text="Technical Details"/>
														</Column>
														<Column id="_IDGenColumn15" width="42.5%">
															<Text id="_IDGenText9" text="Value"/>
														</Column>
														<Column id="_IDGenColumn16" width="15%">
															<Text id="_IDGenText10"
																text="Pref. Req."
																tooltip="Preferred Requirements"
															/>
														</Column>
													</columns>
												</Table>
												<l:VerticalLayout id="_IDGenVerticalLayout2">
													<Label id="_IDGenLabel35"
														text="Country of Origin:"
														class="sapUiTinyMarginTop"
													/>
													<l:HorizontalLayout id="_IDGenHorizontalLayout5">
														<Input
															id="coorBidInput"
															showValueHelp="true"
															valueHelpOnly="true"
															valueHelpRequest=".onCoorValueHelpRequest"
														/>
														<Button
															id="coorBidInfoButton"
															icon="sap-icon://message-information"
															class="sapUiTinyMarginBeginEnd"
															enabled="false"
															tooltip=" "
														/>
													</l:HorizontalLayout>
													<Label id="_IDGenLabel40"
														text="Last Cleaning Date:"
														class="sapUiTinyMarginTop"
													/>
													<l:HorizontalLayout id="_IDGenHorizontalLayout6">
														<DatePicker
															id="lastCleanDateBidInput"
															valueFormat="dd.MM.YYYY"
														/>
														<Button
															id="dateBidInfoButton"
															icon="sap-icon://message-information"
															class="sapUiTinyMarginBeginEnd"
															enabled="false"
															tooltip=" "
														/>
													</l:HorizontalLayout>
													<Label id="_IDGenLabel41"
														text="Last Port of Call:"
														class="sapUiTinyMarginTop"
													/>
													<l:HorizontalLayout id="_IDGenHorizontalLayout7">
														<Input
															id="lastPortBidInput"
															showValueHelp="true"
															valueHelpOnly="true"
															valueHelpRequest=".onPortValueHelpRequest"
														/>
														<Button
															id="portBidInfoButton"
															icon="sap-icon://message-information"
															class="sapUiTinyMarginBeginEnd"
															enabled="false"
															tooltip=" "
														/>
													</l:HorizontalLayout>
													<Label id="_IDGenLabel42"
														text="Demurrage:"
														class="sapUiTinyMarginTop"
													/>
													<l:HorizontalLayout id="_IDGenHorizontalLayout8">
														<Input
															id="demurrageInput"
															showValueHelp="true"
															valueHelpOnly="true"
															valueHelpRequest=".onPortValueHelpRequest"
														/>
														<Button
															id="demurrageInputButton"
															icon="sap-icon://message-information"
															class="sapUiTinyMarginBeginEnd"
															enabled="false"
															tooltip=" "
														/>
													</l:HorizontalLayout>
													<Label id="_IDGenLabel43"
														text="Class of Vessel:"
														class="sapUiTinyMarginTop"
													/>
													<l:HorizontalLayout id="_IDGenHorizontalLayout9">
														<Input
															id="classOfVesselInput"
															showValueHelp="true"
															valueHelpOnly="true"
															valueHelpRequest=".onPortValueHelpRequest"
														/>
														<Button
															id="classofVesselButton"
															icon="sap-icon://message-information"
															class="sapUiTinyMarginBeginEnd"
															enabled="false"
															tooltip=" "
														/>
													</l:HorizontalLayout>
												</l:VerticalLayout>
											
											</l:BlockLayoutCell>
											<l:BlockLayoutCell id="_IDGenBlockLayoutCell6"
												title="Enter Commercial Details"
												titleAlignment="Center"
												titleLevel="Auto"
											>
												<form:SimpleForm id="_IDGenSimpleForm7"
													editable="true"
													backgroundDesign="Transparent"
													width="50%"
												>
													<Label id="_IDGenLabel24"
														text="Freight Cost (Unspecified unit)"
														labelFor="fCost2"
													/>
													<Input
														id="fCost2"
														liveChange="fCostChange"
														type="Number"
													/>
													<Label id="_IDGenLabel25"
														text="Total Cost"
														labelFor="totalCost2"
													/>
													<l:HorizontalLayout id="_IDGenHorizontalLayout3" allowWrapping="false">
														<Input
															id="totalCost2"
															editable="false"
															width="120%"
														/>
														<Label id="_IDGenLabel26"
															text="INR"
															design="Bold"
															labelFor="totalCost2"
															class="sapUiTinyMarginBegin sapUiTinyMarginTop"
														/>
													</l:HorizontalLayout>
												</form:SimpleForm>
												<FlexBox id="_IDGenFlexBox1" justifyContent="End">
													<items>
														<Button
															id="submitBidButton"
															text="Submit Details"
															type="Accept"
															press="onSubmitBid"
														/>
													</items>
												</FlexBox>
											</l:BlockLayoutCell>
										</l:BlockLayoutRow>
									</l:BlockLayout>
								</IconTabFilter>
							</items>
						</IconTabBar>
					</l:BlockLayoutCell>
				</l:BlockLayoutRow>
			</l:BlockLayout>
		</l:VerticalLayout>
	</Page>
</mvc:View>